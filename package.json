    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.15",
    "prisma": "^3.9.1",
    "prisma-erd-generator": "^0.9.0",
    "tap": "^15.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
